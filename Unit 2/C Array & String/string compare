#include "stdio.h"
#include "stdlib.h"
#include "conio.h"
#include "string.h"

int main(void)
{
	char a[10]="ALI";
	char b[10]="ALI";
	int i=0;
	int j;
	while(a[i] != '\0' && b[i] != '\0' )
	{
		if(a[i]==b[i])
		{
			j=0;
			i++;
		}
		else
		{
			j=1;
			break;

		}

	}
	if(j==0)
	{
		printf("Identical \n");

	}
	else
	{
		printf("unidentical \n");
	}



//********************************************//


	if(strcmp(a,b)==0)
		printf("identical");
	return 0;
}
