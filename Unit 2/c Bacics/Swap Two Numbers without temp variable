/*
 *  Swap Two Numbers without temp variable.c
 *
 *  Created on: Jun26,2023
 *  Author: Ali_Reda
 */

#include "stdio.h"
#include "stdlib.h"


int main(void)
{
	float x,y;
	printf("Enter two numbers \n");
	fflush(stdout);
	scanf("%f %f",&x,&y);
	x=x+y;
	y=y-x;
	x=y-x;
	printf("After swapping x = %f \n",x);
	printf("After swapping y = %f",y);

	return 0;
}
